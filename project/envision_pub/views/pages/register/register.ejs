<%- include('../../common/layout-header')-%>
<body>
    <div class="ui-wrap wrap-h bottom-fix two-wrap">
        <!-- header -->
        <header class="ui-header register-header">
            <%- include('../../common/header/header-ver2')-%>
            <h1 class="title">회원가입</h1>
        </header>
        <!-- // header -->
        <div class="ui-container">
            <!-- to dev : 현재페이지 활성화시 li태그에 step-active 클래스 추가 -->
            <!-- to dev : 활성화완료 li태그에 step-on 클래스 추가  -->
            <!-- to dev : 점선 활성화시 dot-img>span태그에 dot-on 클래스 추가 -->
            <div class="member-step">
                <ul class="step-wrap div-wrap">
                    <li class="step step-active">
                        <p>1</p>
                        <p>약관 동의</p>
                    </li>
                    <li class="dot-img">
                        <span></span>
                    </li>
                    <li class="step">
                        <p>2</p>
                        <p>정보 입력</p>
                    </li>
                    <li class="dot-img">
                        <span></span>
                    </li>
                    <li class="step">
                        <p>3</p>
                        <p>가입 완료</p>
                    </li>
                </ul>
            </div>
            <div class="register div-wrap reset">
                <div class="check-wrap p-side-15">
                    <ul>
                        <li>
                            <input type="checkbox" name="checker" id="checker1" checked/>
                            <label for="checker1">췌키 이용약관(필수)</label>
                            <a href="#" class="more">상세보기</a>
                        </li>
                        <li>
                            <input type="checkbox" name="checker" id="checker2" checked/>
                            <label for="checker2">개인정보처리방침(필수)</label>
                            <a href="#" class="more">상세보기</a>
                        </li>
                        <li>
                            <input type="checkbox" name="checker" id="checker3" checked/>
                            <label for="checker3">개인정보 수집 이용 동의(필수)</label>
                            <a href="#" class="more">상세보기</a>
                        </li>
                    </ul>
                </div>
                <div class="check-wrap all">
                    <ul>
                        <li>
                            <input type="checkbox" name="checker" id="checker4" checked/>
                            <label for="checker4">약관 전체 동의</label>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="btn-box two-btn-wrap p-side-20">
            <button type="button" class="btn gray-btn">취소</button>
            <button type="button" class="btn navy-btn">다음 단계</button>
        </div>
    </div>
</body>
</html>
